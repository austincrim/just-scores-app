{
  "expo": {
    "name": "just scores",
    "slug": "just-scores",
    "owner": "austincrim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/apple/1024.png",
    "scheme": "justscores",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": ["ios"],
    "plugins": [
      [
        "expo-widgets",
        {
          "groupIdentifier": "group.com.austincrim.justscores",
          "widgets": [
            {
              "name": "GameScoreActivity",
              "displayName": "Live Score",
              "description": "Track live game scores in real-time",
              "supportedFamilies": ["accessoryRectangular", "accessoryInline"]
            }
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.austincrim.justscores",
      "appleTeamId": "5YQ2CG7528",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "extra": {
      "eas": {
        "projectId": "d445fa8e-4fbb-450f-a141-a83243d8dcb0"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d445fa8e-4fbb-450f-a141-a83243d8dcb0"
    },
    "android": {}
  }
}
